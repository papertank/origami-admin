{
  "name": "origami/admin",
  "description": "Laravel 5 Admin Package",
  "authors": [
    {
      "name": "Papertank",
      "email": "david@papertank.co.uk"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/auth": "~5.4|^6.0",
    "illuminate/support": "~5.4|^6.0",
    "illuminate/routing": "~5.4|^6.0",
    "laravelcollective/html": "~5.4|^6.0",
    "origami/notice": "^1.2",
    "lab404/laravel-impersonate": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Origami\\Admin\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "2.1.x-dev"
    },
    "laravel": {
      "providers": [
        "Origami\\Admin\\AdminServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
